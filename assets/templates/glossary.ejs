<dl class="list">


<% for (const item of items) { %>
<div class="quarto-post" <%= metadataAttrs(item) %> >
<dt class="listing-name glossary-item-title">
<%= item.name %><br />
<code><span class="badge rounded-pill bg-info text-light"><%= item.data_type %></span></code>
</dt>
<dd class="listing-description">
<span class="listing-categories">
<span class="listing-category float-end"><%= item.categories %></span>
</span>
<%= item.description %>
</dd>
</div>
<% } %>

</dl>
